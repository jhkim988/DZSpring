<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Post">
	<select id="findByCategoryLimit10" resultType="com.dzspring.app.entity.Post">
		select
			*
		from tb_dzboard_board
		where
			category = #{category}
		order by id desc
		limit 10
	</select>
</mapper>